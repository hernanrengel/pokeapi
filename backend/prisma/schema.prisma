// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model Favorite {
  id        String   @id @default(uuid())
  pokemonId Int
  createdAt DateTime @default(now())

  @@map("favorites")
}
